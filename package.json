{
  "name": "@xakpc/gulp-grass-sass",
  "version": "0.1.0",
  "main": "main.js",
  "types": "main.d.ts",
  "napi": {
    "name": "gulp-grass-sass",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "author": "xakpc",
  "description": "Gulp plugin for compiling Sass files using grass Rust library",
  "dependencies": {
    "plugin-error": "2.0.1"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.3",
    "ava": "^6.0.1",
    "gulp": "^5.0.0",
    "gulp-buffer": "0.0.2"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "optionalDependencies": {
    "@xakpc/gulp-grass-sass-win32-arm64-msvc": "0.1.0",
    "@xakpc/gulp-grass-sass-win32-x64-msvc": "0.1.0",
    "@xakpc/gulp-grass-sass-win32-ia32-msvc": "0.1.0"
  }
}
